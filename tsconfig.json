{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "components/*": ["src/components/*"],
      "ui/*": ["src/ui/*"],
      "types/*": ["src/types/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
